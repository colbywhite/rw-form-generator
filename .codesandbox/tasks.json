{
  "$schema": "https://codesandbox.io/schemas/tasks.json",
  "setupTasks": [
    "yarn install",
    "yarn rw prisma migrate dev"
  ],
  "tasks": {
    "dev": {
      "name": "yarn rw dev",
      "command": "yarn rw dev --fwd=\"--allowed-hosts all\"",
      "runAtStart": true,
      "preview": {
        "port": 8910
      }
    }
  }
}
